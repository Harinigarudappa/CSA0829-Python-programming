def isValid(s: str) -> bool:
    stack = []
    mapping = {')': '(', '}': '{', ']': '['}
    
    for char in s:
        if char in mapping:
            top_element = stack.pop() if stack else '#'
            if mapping[char] != top_element:
                return False
        else:
            stack.append(char)
    
    return not stack

# User input
user_input = input("Enter a string containing only brackets: ")
print("Input string:", user_input)
print("Is valid:", isValid(user_input))
